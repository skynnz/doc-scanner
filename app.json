{
  "expo": {
    "name": "doc-scanner",
    "slug": "doc-scanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "doc-scanner",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fabrizzio.docscanner"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET"
      ],
      "package": "com.skynnz.docscanner"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-file-system",
        {
          "foregroundPermissions": [
            "READ_EXTERNAL_STORAGE",
            "WRITE_EXTERNAL_STORAGE"
          ]
        }
      ],
      [
        "expo-document-picker"
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "adobeClientId": "7580591ed92c4417bbb73240d951d3b7",
      "adobeClientSecret": "p8e-f7q_f3_UwE3u6V4Q-sh04wt9IFU3Jvfl",
      "AdobeOrg": "480F206267F5CD000A495FE2@AdobeOrg",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f244068c-e888-496a-a529-ef59b14e3415"
      }      
    },
    "owner": "skynnz"
  }
}
